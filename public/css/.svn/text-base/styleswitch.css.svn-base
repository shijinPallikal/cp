#styleswitch {
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #CCCCCC;
    color: #CCCCCC;
    height: 188px;
    left: -200px;
    padding: 0 5px 5px;
    position: fixed;
    top: 200px;
    width: 200px;
    z-index: 9999;
}
#styleswitch h3 {
    background: none repeat scroll 0 0 #8DC153;
    border-radius: 5px;
    color: #FFFFFF;
    cursor: pointer;
    margin: -2px 0 5px -10px;
    padding: 5px 10px 5px 20px;
    width: 250px;
}
#styleswitch h3 i {
    margin: 0 5px;
}
#styleswitch a {
    display: inline-block;
    height: 22px;
    margin: 6px;
    text-indent: -9999px;
    width: 22px;
}
#styleswitch a#color1 {
    background: none repeat scroll 0 0 #F25C27;
}
